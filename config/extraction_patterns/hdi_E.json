{
  "Police_No": {
    "pattern": "(\\d{13}/\\d+/\\d+)",
    "group": 1,
    "description": "Policy number in format XXXXXXXXXXXXX/0/0"
  },
  "Police_Onceki_No": {},
  "Police_Baslangic_Tarihi": {
    "pattern": "BAŞLANGIÇ-BİTİŞ TARİHİ\\s*\\n([0-9]{2}/[0-9]{2}/[0-9]{4})-([0-9]{2}/[0-9]{2}/[0-9]{4})",
    "group": 1,
    "description": "Policy start date from date range field"
  },
  "Police_Bitis_Tarihi": {
    "pattern": "BAŞLANGIÇ-BİTİŞ TARİHİ\\s*\\n([0-9]{2}/[0-9]{2}/[0-9]{4})-([0-9]{2}/[0-9]{2}/[0-9]{4})",
    "group": 2,
    "description": "Policy end date from date range field"
  },
  "Sigortali_TC_Kimlik_No": {
    "pattern": "T\\.C\\.KİMLİK NO\\s*\\n:\\s*([^\\n]+)",
    "group": 1,
    "description": "TC identity number (personal customers only, masked)"
  },
  "Sigortali_Adi": {
    "pattern": "AD SOYAD / ÜNVAN\\s*\\n:\\s*([^\\n]+)",
    "group": 1,
    "description": "Customer name (personal) or company name (corporate)"
  },
  "Sigortali_Adresi": {
    "pattern": "(?:T\\.C\\.KİMLİK NO|VERGİ NO)\\s*\\n:[^\\n]+\\nADRESİ\\s*\\n:\\s*([^\\n]+)",
    "group": 1,
    "description": "Customer address after TC/tax number"
  },
  "Sigortali_Eposta": {},
  "Sigortali_GSM": {
    "pattern": "TELEFON NO/EMAIL\\s*\\n:\\s*([0-9\\*]+)",
    "group": 1,
    "description": "Customer phone number (masked format)"
  },
  "Sigortali_Meslegi": {},
  "Sigortali_Musteri_No": {},
  "Sigortali_Vergi_No": {},
  "SBM_Tramer_Belge_No": {},
  "SBM_Bim_Reference_No": {},
  "Arac_Plaka_No": {
    "pattern": "Plaka No\\s*\\n:\\s*([^\\n]+)",
    "group": 1,
    "description": "Vehicle license plate number"
  },
  "Arac_Motor_No": {
    "pattern": "Motor No\\s*\\n:\\s*([^\\n]+)",
    "group": 1,
    "description": "Vehicle engine number"
  },
  "Arac_Sasi_No": {
    "pattern": "Şasi No\\s*\\n:\\s*([^\\n]+)",
    "group": 1,
    "description": "Vehicle chassis number (VIN)"
  },
  "Arac_Marka_Tip": {
    "pattern": "RİSK BİLGİLERİ[\\s\\S]*?Marka\\s*\\n:\\s*([^\\n]+)",
    "group": 1,
    "description": "Vehicle brand from risk information section"
  },
  "Arac_Model": {
    "pattern": "Tipi\\s*\\n:\\s*([^\\n]+(?:\\n[A-Z0-9\\s]*?)?)(?=\\nŞasi No)",
    "group": 1,
    "description": "Vehicle model/type (e.g., MODEL Y STANDART RANGE RWD FL)"
  },
  "Arac_Model_Yili": {
    "pattern": "RİSK BİLGİLERİ[\\s\\S]*?Model\\s*\\n:\\s*([^\\n]+)",
    "group": 1,
    "description": "Vehicle model year (e.g., 2025)"
  },
  "Arac_Kullanim_Sekli": {
    "pattern": "Alt Kullanım Tarzı\\s*\\n:\\s*([^\\n]+)",
    "group": 1,
    "description": "Vehicle usage type (HUSUSİ, TİCARİ, etc.)"
  },
  "Arac_Marka_Kodu": {},
  "Arac_Engelli_Mi": {
    "pattern": "Engelli Aracı Mı\\?\\s*\\n:\\s*([^\\n]+)",
    "group": 1,
    "description": "Disability vehicle status (HAYIR/EVET)"
  },
  "Teminat_Oto_Cam_Klozu": {
    "pattern": "Cam Anlaşmalı Yetkili\\s*\\n(Dâhil|DAHİL)",
    "group": 1,
    "description": "Glass coverage through contracted services"
  },
  "Teminat_IMM_Bedeni_Maddi_Ayrimsiz_Bedeli": {
    "pattern": "İmm - Maddi Ve Bedeni\\s*\\n([0-9.,]+ TL)",
    "group": 1,
    "description": "IMM combined coverage amount"
  },
  "Teminat_IMM_Manevi_Tazminat_Bedeli": {
    "pattern": "Manevi Tazminat\\s*\\n([0-9.,]+ TL)",
    "group": 1,
    "description": "Moral damages coverage amount"
  },
  "Teminat_Indirim_Arttırim_Bilgileri": {
    "pattern": "Hasarsızlık Koruma\\s*\\n(Dâhil|DAHİL)",
    "group": 1,
    "description": "No-claims protection coverage availability"
  },
  "Teminat_Ikame_Arac": {
    "pattern": "İkame Araç Gün Sayısı\\s*\\n:\\s*İKAME\\s*\\([^)]+\\)\\s*(\\d+)X(\\d+)\\s*GÜN",
    "group": 1,
    "description": "Replacement vehicle days (first number from XxY pattern)"
  },
  "Teminat_Yedek_Parca": {
    "pattern": "YEDEK PARÇA KLOZU",
    "group": 0,
    "description": "Original parts coverage availability (presence indicates coverage)"
  },
  "Teminat_Hasar_Kademesi_Bilgisi": {
    "pattern": "Hasarsızlık Kademesi\\s*\\n:\\s*([^\\n]+)",
    "group": 1,
    "description": "No-claims discount step level (X. BASAMAK or İNDİRİMSİZ)"
  },
  "Teminat_Hasarsizlik_Koruma": {},
  "Police_Gross_Price": {
    "pattern": "BRÜT PRİM\\s*:\\s*([0-9,\\.]+)",
    "group": 1
  }
}
