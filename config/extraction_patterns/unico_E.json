{
  "Police_No": {
    "pattern": "(\\d{9})\\s*/\\s*\\d+",
    "group": 1,
    "description": "Policy number - 9-digit format"
  },
  "Police_Onceki_No": {
    "pattern": "Ö\\.POLİÇE NO[\\s\\S]*?:\\s*(\\d{8,})",
    "group": 1,
    "description": "Previous policy number"
  },
  "Police_Baslangic_Tarihi": {
    "pattern": "BAŞLAMA TARİHİ[\\s\\S]*?(\\d{2}/\\d{2}/\\d{4})",
    "group": 1,
    "description": "Policy start date in DD/MM/YYYY format"
  },
  "Police_Bitis_Tarihi": {
    "pattern": "BİTİŞ TARİHİ[\\s\\S]*?(\\d{2}/\\d{2}/\\d{4})",
    "group": 1,
    "description": "Policy end date in DD/MM/YYYY format"
  },
  "Sigortali_TC_Kimlik_No": {
    "pattern": "TCK/VERGİ DR\\. NO[\\s\\S]*?:\\s*(\\d{2}\\*+\\d{2})",
    "group": 1,
    "description": "Masked TC identity number"
  },
  "Sigortali_Adi": {
    "pattern": "([A-ZÇĞIİÖŞÜ\\s]+)\\s*SİGORTALI ADI SOYADI/ÜNVANI",
    "group": 1,
    "description": "Customer name appearing before SIGORTALI ADI field"
  },
  "Sigortali_Eposta": {},
  "Sigortali_GSM": {
    "pattern": "(\\d{2}\\*+\\d{2})\\s*/\\s*:[\\s\\S]*?GSM / TEL",
    "group": 1,
    "description": "Masked GSM phone number"
  },
  "Sigortali_Meslegi": {
    "pattern": "beyan edilen meslek;\\s*([^\\s][^;]+?)\\s*olarak seçilmiştir",
    "group": 1,
    "description": "Customer profession"
  },
  "Sigortali_Musteri_No": {
    "pattern": "BİTİŞ TARİHİ[\\s\\S]*?\\d{2}/\\d{2}/\\d{4}[\\s\\S]*?(\\d{7})",
    "group": 1,
    "description": "Customer number - 7-digit format"
  },
  "Sigortali_Vergi_No": {},
  "SBM_Tramer_Belge_No": {},
  "SBM_Bim_Reference_No": {},
  "Arac_Plaka_No": {
    "pattern": "PLAKA[\\s\\S]*?:\\s*([A-Z0-9]{2,3}\\s*[A-Z0-9]{2,6})",
    "group": 1,
    "description": "License plate in XX XXXXXX format"
  },
  "Arac_Motor_No": {
    "pattern": "MOTOR NO[\\s\\S]*?:\\s*([A-Z0-9]+)",
    "group": 1,
    "description": "Vehicle motor number"
  },
  "Arac_Sasi_No": {
    "pattern": "ŞASİ NO[\\s\\S]*?:\\s*([A-Z0-9]+)",
    "group": 1,
    "description": "Vehicle chassis number"
  },
  "Arac_Marka_Tip": {
    "pattern": "MARKA[\\s\\S]*?:\\s*([A-ZÇĞIİÖŞÜ]+)",
    "group": 1,
    "description": "Vehicle brand name"
  },
  "Arac_Model": {
    "pattern": "TİP[\\s\\S]*?:\\s*([A-Z0-9\\.\\s]+?)(?=\\n|$|\\s{2,})",
    "group": 1,
    "description": "Vehicle model/type"
  },
  "Arac_Model_Yili": {
    "pattern": "MODEL[\\s\\S]*?:\\s*(20\\d{2})",
    "group": 1,
    "description": "Vehicle model year"
  },
  "Arac_Kullanim_Sekli": {
    "pattern": "KULLANIM TARZI[\\s\\S]*?:\\s*\\d+\\s*/\\s*([^\\n]+)",
    "group": 1,
    "description": "Vehicle usage type"
  },
  "Arac_Marka_Kodu": {},
  "Arac_Engelli_Mi": {},
  "Teminat_Oto_Cam_Klozu": {
    "pattern": "(orjinal olması koşuluyla orjinal cam\\s*\\nkullanılacaktır)",
    "group": 1,
    "description": "Glass coverage clause - original glass will be used"
  },
  "Teminat_IMM_Bedeni_Maddi_Ayrimsiz_Bedeli": {
    "pattern": "İHT\\.MALİ SORUMLULUK[\\s\\S]*?([\\d\\.,]+)",
    "group": 1,
    "description": "IMM coverage amount"
  },
  "Teminat_IMM_Manevi_Tazminat_Bedeli": {
    "pattern": "MANEVİ TAZMİNAT TEMİNATI[\\s\\S]*?([\\d\\.,]+)",
    "group": 1,
    "description": "Moral compensation coverage amount"
  },
  "Teminat_Indirim_Arttırim_Bilgileri": {
    "pattern": "HASARSIZLIK KORUMA\\s*\\n([^\\n]+)",
    "group": 1,
    "description": "No-claims discount protection - indicates if included"
  },
  "Teminat_Ikame_Arac": {
    "pattern": "teminat limitlerinde belirtilen\\s*(\\d+X\\d+\\s*GÜN\\s*STANDART)",
    "group": 1,
    "description": "Replacement vehicle coverage - captures NxM GÜN STANDART format"
  },
  "Teminat_Yedek_Parca": {
    "pattern": "hasarın tazmininde\\s*(logolu veya[\\s\\n]*logosuz[\\s\\n]*orijinal[\\s\\n]*parça)",
    "group": 1,
    "description": "Spare parts coverage - branded or unbranded original parts"
  },
  "Teminat_Hasar_Kademesi_Bilgisi": {
    "pattern": "HASARSIZLIK İND\\s*\\n:\\s*(\\d+\\.BASAMAK)",
    "group": 1,
    "description": "No-claims discount level/step"
  },
  "Police_Gross_Price": {
    "pattern": "BRÜT PRİM\\s*\\s*([0-9,\\.]+)",
    "group": 1
  }
}
