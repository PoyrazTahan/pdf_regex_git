{
  "Police_No": {
    "pattern": "Poliçe/.*?No.*?([0-9]{8,})",
    "group": 1,
    "description": "Policy number extraction"
  },
  "Police_Onceki_No": {
    "pattern": "Önceki Poliçe No\\n:\\s*([0-9]+)",
    "group": 1,
    "description": "Previous policy number"
  },
  "Police_Baslangic_Tarihi": {
    "pattern": "Başlangıç Tarihi.*?:\\s*([0-9]{2}/[0-9]{2}/[0-9]{4})",
    "group": 1,
    "description": "Policy start date"
  },
  "Police_Bitis_Tarihi": {
    "pattern": "Bitiş Tarihi.*?:\\s*([0-9]{2}/[0-9]{2}/[0-9]{4})",
    "group": 1,
    "description": "Policy end date"
  },
  "Sigortali_TC_Kimlik_No": {
    "pattern": "T\\.C\\. Kimlik No\\n:\\s*([*0-9]+)",
    "group": 1,
    "description": "Customer TC identity number (masked)"
  },
  "Sigortali_Adi": {
    "pattern": "Adı Soyadı.*?Ünvanı.*?:\\s*([A-ZÇĞIİÖŞÜa-zçğıiöşü\\s]+?)\\n",
    "group": 1,
    "description": "Customer name"
  },
  "Sigortali_Adresi": {
    "pattern": "Sigortalı Adresi\\n:\\s*([\\s\\S]*?)\\nT\\.C\\. Kimlik",
    "group": 1,
    "description": "Customer address (multi-line)"
  },
  "Sigortali_Eposta": {},
  "Sigortali_GSM": {
    "pattern": "GSM No\\n:\\s*([*0-9\\s]+?)\\n",
    "group": 1,
    "description": "Customer phone number (masked)"
  },
  "Sigortali_Meslegi": {
    "pattern": "Yenileme No\\n:\\s*[0-9]+\\n([A-ZÇĞIİÖŞÜa-zçğıiöşü\\s]+?)\\nHasarsızlık",
    "group": 1,
    "description": "Customer profession"
  },
  "Sigortali_Musteri_No": {
    "pattern": "Sigortalı No.*?:\\s*([0-9]+)",
    "group": 1,
    "description": "Customer number"
  },
  "Sigortali_Vergi_No": {},
  "SBM_Tramer_Belge_No": {},
  "SBM_Bim_Reference_No": {},
  "Arac_Plaka_No": {
    "pattern": "Plaka No.*?:\\s*([A-Z0-9\\s]+?)\\n",
    "group": 1,
    "description": "Vehicle license plate"
  },
  "Arac_Motor_No": {
    "pattern": "Motor No\\n:\\s*([A-Z0-9]+)",
    "group": 1,
    "description": "Vehicle motor number"
  },
  "Arac_Sasi_No": {
    "pattern": "Şasi No\\n:\\s*([A-Z0-9]+)",
    "group": 1,
    "description": "Vehicle chassis number"
  },
  "Arac_Marka_Tip": {
    "pattern": "Marka\\n:\\s*([A-ZÇĞIİÖŞÜa-zçğıiöşü]+)\\n",
    "group": 1,
    "description": "Vehicle brand"
  },
  "Arac_Model": {
    "pattern": "Tipi\\n:\\s*([A-ZÇĞIİÖŞÜa-zçğıiöşü0-9\\s\\.]+?)\\n",
    "group": 1,
    "description": "Vehicle model/type"
  },
  "Arac_Model_Yili": {
    "pattern": "Model\\n:\\s*([0-9]{4})\\n",
    "group": 1,
    "description": "Vehicle model year"
  },
  "Arac_Kullanim_Sekli": {
    "pattern": "Kullanım Tarzı\\n:\\s*([A-ZÇĞIİÖŞÜa-zçğıiöşü\\s/]+?)\\n",
    "group": 1,
    "description": "Vehicle usage type"
  },
  "Arac_Marka_Kodu": {},
  "Arac_Engelli_Mi": {},
  "Teminat_Oto_Cam_Klozu": {
    "patterns": [
      "(Herhangi\\s+bir\\s+cam[\\s\\S]*?muafiyet\\s+uygulanmayacaktır\\.)",
      "(Herhangi\\s+bir\\s+cam[\\s\\S]*?%\\s*\\d{1,2}\\s*['’]?s[ıi]\\s*ve\\s*(?:min(?:imum)?|asgari)\\s*\\d{1,3}(?:\\.\\d{3})*\\s*TL[\\s\\S]*?muafiyet\\s+uygulanacaktır\\.)",
      "(Herhangi\\s+bir\\s+cam[\\s\\S]*?(?:asgari|min(?:imum)?)\\s*\\d{1,3}(?:\\.\\d{3})*\\s*TL[\\s\\S]*?%\\s*\\d{1,2}\\s*(?:oranında)?[\\s\\S]*?muafiyet\\s+uygulanacaktır\\.)"
    ],
    "mode": "first",
    "group": 1,
    "description": "1) Muafiyetsiz cam cümlesi. 2) ‘%… ve minimum/asgari … TL … uygulanacaktır.’ sırası. 3) ‘asgari … TL … %… oranında … uygulanacaktır.’ sırası."
  },
  "Teminat_IMM_Bedeni_Maddi_Ayrimsiz_Bedeli": {
    "pattern": "İHTİYARİ MADDİ BEDENİ AYRIMI YAPILMAKSIZIN\\n([0-9,\\.]+)",
    "group": 1,
    "description": "IMM coverage amount"
  },
  "Teminat_IMM_Manevi_Tazminat_Bedeli": {
    "pattern": "manevi tazminat.*?azami.*?%([0-9]+)",
    "group": 1,
    "description": "Manevi tazminat coverage percentage"
  },
  "Teminat_Indirim_Arttırim_Bilgileri": {
    "pattern": "Hasarsızlık indirimi\\n:\\s*([%0-9\\s]+[A-ZÇĞIİÖŞÜa-zçğıiöşü]+)",
    "group": 1,
    "description": "Discount information"
  },
  "Teminat_Ikame_Arac": {
    "pattern": "(maksimum \\d+ gün ile sınırlıdır|ikame araç teminatı bulunmamaktadır)",
    "group": 1,
    "description": "Replacement vehicle coverage - duration limit or excluded"
  },
  "Teminat_Yedek_Parca": {
    "pattern": "(eşdeğer\\nve/veya muadil parça kullanılacaktır)",
    "group": 1,
    "description": "Spare parts policy - equivalent/similar parts used with customer approval"
  },
  "Teminat_Hasar_Kademesi_Bilgisi": {},
  "Police_Gross_Price": {
    "pattern": "TOPLAM PRİM\\s*:\\s*([0-9,\\.]+)",
    "group": 1
  }
}
