{
  "Police_No": {
    "pattern": "(?:Poliçe No.*?\\n.*?\\n.*?\\n.*?\\n)(\\d{7})",
    "group": 1,
    "description": "Policy number from table - 7 digits"
  },
  "Police_Onceki_No": {},
  "Police_Baslangic_Tarihi": {
    "pattern": "Başlangıç Tarihi\\s+Bitiş Tarihi[\\s\\S]*?\\b\\d{2}[./]\\d{2}[./]\\d{4}\\b\\s+(\\d{2}[./]\\d{2}[./]\\d{4})",
    "group": 1,
    "description": "Tabloda tanzim tarihinden sonraki (2.) tarihi alır"
  },
  "Police_Bitis_Tarihi": {
    "pattern": "Başlangıç Tarihi\\s+Bitiş Tarihi[\\s\\S]*?\\b\\d{2}[./]\\d{2}[./]\\d{4}\\b\\s+\\d{2}[./]\\d{2}[./]\\d{4}\\s+(\\d{2}[./]\\d{2}[./]\\d{4})",
    "group": 1,
    "description": "Tabloda 3. tarihi (bitiş) alır"
  },
  "Sigortali_TC_Kimlik_No": {
    "pattern": "T\\.C\\. Kimlik No: (\\*+\\d+)",
    "group": 1,
    "description": "TC Identity number (masked)"
  },
  "Sigortali_Adi": {
    "pattern": "Katılımcı: ([A-ZÇĞIİÖŞÜ\\s]+?)\\s+Adresi",
    "group": 1,
    "description": "Customer name"
  },
  "Sigortali_Adresi": {
    "pattern": "Katılımcı: .+?\\nAdresi : (.+?)\\nCep Telefonu:",
    "group": 1,
    "description": "Customer address"
  },
  "Sigortali_Eposta": {},
  "Sigortali_GSM": {
    "pattern": "Cep Telefonu: (\\*+\\d+)",
    "group": 1,
    "description": "Customer phone (masked)"
  },
  "Sigortali_Meslegi": {},
  "Sigortali_Musteri_No": {},
  "Sigortali_Vergi_No": {},
  "SBM_Tramer_Belge_No": {},
  "SBM_Bim_Reference_No": {},
  "Arac_Plaka_No": {
    "pattern": "Plaka\\s*:\\s*([\\d]{2,3}\\s+[A-Z]+\\d+)",
    "group": 1,
    "description": "Vehicle license plate"
  },
  "Arac_Motor_No": {
    "pattern": "Motor No.*?:\\s*([A-Z0-9]+)",
    "group": 1,
    "description": "Engine number"
  },
  "Arac_Sasi_No": {
    "pattern": "Şasi No.*?:\\s*([A-Z0-9]+)",
    "group": 1,
    "description": "Chassis number"
  },
  "Arac_Marka_Tip": {
    "pattern": "Marka.*?:\\s*([A-Z]+)",
    "group": 1,
    "description": "Vehicle brand"
  },
  "Arac_Model": {
    "pattern": "Tip\\s*:\\s*([^\\n]+)",
    "group": 1,
    "description": "Vehicle model"
  },
  "Arac_Model_Yili": {
    "pattern": "Model Yılı\\s*:\\s*(\\d{4})",
    "group": 1,
    "description": "Vehicle model year"
  },
  "Arac_Kullanim_Sekli": {
    "pattern": "Kullanım Tarzı\\s*:\\s*([^\\n]+)",
    "group": 1,
    "description": "Vehicle usage type"
  },
  "Arac_Marka_Kodu": {},
  "Arac_Engelli_Mi": {},
  "Teminat_Oto_Cam_Klozu": {
    "pattern": "Cam Servis Kapsamı: ([^\\n]+)",
    "group": 1,
    "description": "Glass service coverage"
  },
  "Teminat_IMM_Bedeni_Maddi_Ayrimsiz_Bedeli": {
    "pattern": "IMM KOMBİNE\\s*\\n([\\d,\\.]+)",
    "group": 1,
    "description": "IMM combined coverage amount"
  },
  "Teminat_IMM_Manevi_Tazminat_Bedeli": {
    "pattern": "IMM MANEVİ TAZMİNAT TALEPLERİ.*?([\\d,\\.]+)",
    "group": 1,
    "description": "IMM moral compensation amount"
  },
  "Teminat_Indirim_Arttırim_Bilgileri": {
    "pattern": "(Hasarsızlık İndirim Oranı : %[0-9]+|HASARSIZLIK KORUMA TEMİNATI)",
    "group": 1,
    "description": "Discount information - no-claims discount rate or protection coverage status"
  },
  "Teminat_Ikame_Arac": {
    "pattern": "İkame Araç Süresi : ([^\\n]+)",
    "group": 1,
    "description": "Replacement vehicle duration and segment (e.g., C Segment 7 gün(Yılda 2 kez) or Yok)"
  },
  "Teminat_Yedek_Parca": {
    "pattern": "ARAÇ ONARIM SERVİSİ VE TEDARİK PARÇA TÜRÜ: (ORJİNAL PARÇA)",
    "group": 1,
    "description": "Spare parts type - original parts specified for all services"
  },
  "Teminat_Hasar_Kademesi_Bilgisi": {
    "pattern": "Hasarsızlık İndirim Oranı : (%?\\d+)",
    "group": 1,
    "description": "No-claim discount rate"
  },
  "Police_Gross_Price": {
    "pattern": "Brüt Katkı Payı\\s*\\s*([0-9,\\.]+)",
    "group": 1
  }
}
